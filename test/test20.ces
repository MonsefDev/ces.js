#appendButton $click #appendParagraph {
    text: += "appended content ";
}

#appendHTMLButton $click #appendHTMLParagraph {
    html: += " <strong>new html</strong>";
}

#multipleAppend $click {
    text: += " new";
    text: += " text";
}

#multipleHTMLAppend $click {
    html: += " <em>new</em>";
    html: += " <strong>text</strong>";
}

#quote $click {
    text: "Quote \" value";
}

#quoteHTML $click {
    html: "Quote <strong>\"</strong> value";
}

#prependButton $click #prependParagraph {
    text: =+ "prepended content ";
}

#prependHTMLButton $click #prependHTMLParagraph {
    html: =+ "<strong>prepend</strong> ";
}
