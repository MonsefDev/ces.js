#increment-size $click #increment-div {
    border-bottom-width: +=1px;
    border-left-width: +=1px;
    border-right-width: +=1px;
    border-top-width: +=1px;
    width: += 10px;
}

#decrement-size $click #increment-div {
    border-bottom-width: -=1px;
    border-left-width: -=1px;
    border-right-width: -=1px;
    border-top-width: -=1px;
    width: -= 10px;
}
