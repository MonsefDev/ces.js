button#button $click {
    background-color: blue;
    color: red;
    text: "Clicked";
}

//Change the paragraph HTML on click.
#paragraph $click {
    background-image: url('image.png');
    html: "<strong>Text</strong>";
}

#show $click #par {
    background-image: url("image.png");
    display: block;
}
